<table>
  <tr>
    <th>name</th>
    <th>email</th>
    <th>age</th>
    <th>address</th>
    <th>birthday</th>
    <th>over_limit</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.age %></td>
    <td><%= user.address %></td>
    <td><%= user.birthday %></td>
    <td><%= user.flg_over_limit %></td>
  </tr>
<% end %>
</table>